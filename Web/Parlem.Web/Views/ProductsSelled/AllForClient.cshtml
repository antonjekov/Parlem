@model IEnumerable<Parlem.Web.ViewModels.ProductSelled.ProductSelledViewModel>

@{
    this.ViewData["Title"] = "Products";
}

<div class="container">
    <div class="row">
        <div class="col-sm-6 offset-sm-3">
            <h1>@this.ViewData["Title"]</h1>

@foreach (var item in Model)
{
    <p>
    <div>_id: @item._id</div>
    <div>productName: @item.ProductName</div>
    <div>productTypeName: @item.ProductTypeName</div>
    <div>numeracioTerminal: @item.NumeracioTerminal</div>
    <div>soldAt: @item.SoldAt</div>
    <div>customerId: @item.CustomerId</div>
    </p>
}

@if (@Model.ToList().Count==0)
{
    <div>Client with this document number don't have contracts.</div>
}
        </div>
    </div>
</div>



