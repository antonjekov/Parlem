@model Parlem.Web.ViewModels.Clients.ClientViewModel

@{
    this.ViewData["Title"] = "Client";
}

<div class="container">
    <div class="row">
        <div class="col-sm-6 offset-sm-3">
            <h1>@this.ViewData["Title"]</h1>

            @if (@Model!=null)
           {
               <p>
    <div>_id: @Model._id</div>
    <div>DocType: @Model.DocType</div>
    <div>DocNum: @Model.DocNum</div>
    <div>Email: @Model.Email</div>
    <div>CustomerId: @Model.CustomerId</div>
    <div>GivenName: @Model.GivenName</div>
    <div>FamilyName1: @Model.FamilyName1</div>
    <div>Phone: @Model.Phone</div>
                </p>
            }
            else
            {
               <div>There is no client with this document number</div>
            }   
        </div>
    </div>
</div>